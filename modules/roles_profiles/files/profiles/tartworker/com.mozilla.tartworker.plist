<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.mozilla.tartworker</string>
  <key>ProgramArguments</key>
  <array>
    <string>/opt/homebrew/bin/tart</string>
    <string>run</string>
    <string>--auto-login</string>
    <string>--randomize-identifiers</string>
  </array>
  <key>RunAtLoad</key><true/>
  <key>KeepAlive</key><true/>
  <key>StandardOutPath</key><string>/var/log/tartworker.out</string>
  <key>StandardErrorPath</key><string>/var/log/tartworker.err</string>
</dict>
</plist>
